@font-face {
  font-family: 'icomoon';
  src: url('data:font/truetype;charset=utf-8;base64,') format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
[class^="icon-"], [class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'icomoon' !important;
  /* speak: never; */
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.container1{
  padding: 50rpx 40rpx;
  box-sizing: border-box;
}
/* 班级头像 */
.classImg{
  height: 152rpx;
  background: #f9f9ff;
  border-radius: 20rpx;
  border: solid 1px #f5f7ff;
  padding: 30rpx;
  box-sizing: border-box;
  color: #afb6d3;
  font-family: "PingFangSC-Regular";
  font-size: 32rpx;
  position: relative;
}
.classImg image{
  width: 80rpx;
  height: 80rpx;
  vertical-align: middle;
  margin-right: 6rpx;
}
/* 班级名称 */
.className{
  margin-top: 30rpx;
  height: 176rpx;
  background: #f9f9ff;
  border-radius: 20rpx;
  border: 1px solid #f5f7ff;
  font-family: "PingFangSC-Regular";
  padding: 30rpx;
  box-sizing: border-box;
  color: #afb6d3;
  font-size: 32rpx;
  position: relative;
}
.classNameNum{
  color: #071756;
  font-size: 32rpx;
  font-family: "PingFangSC-Regular";
  margin-top: 16rpx;
  overflow : hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
/* 班级描述 */
.classDesc{
  height: 230rpx;
  background: #f9f9ff;
  border-radius: 20rpx;
  border: 1px solid #f5f7ff;
  margin-top: 30rpx;
  font-size: 32rpx;
  color: #afb6d3;
  font-family: "PingFangSC-Regular";
  padding: 30rpx;
  box-sizing: border-box;
  position: relative;
}
/* 生成二维码 */
.classCode{
  position: relative;
  height: 94rpx;
  background: #f9f9ff;
  border-radius: 20rpx;
  border: 1px solid #f5f7ff;
  margin-top: 30rpx;
  font-size: 32rpx;
  color: #afb6d3;
  font-family: "PingFangSC-Regular";
  padding: 20rpx;
  box-sizing: border-box;
}
/* 按钮 */
.btnForm{
  width: 670rpx;
  height: 72rpx;
  position: fixed;
  bottom: 20rpx;
  left: 40rpx;
  background: #5276f2;
  border-radius: 40rpx;
  text-align: center;
  line-height: 72rpx;
  color: #fff;
  font-size: 28rpx;
  font-family: "PingFangSC-Medium";
}
/* 弹窗 */
.desc{
  position: fixed;
  bottom: 0;
  left: 31rpx;
  height: 100%;
  border-radius: 40rpx 40rpx 0 0;
  background: #fff;
}
.title_d{
  display: flex;
  text-align: center;
}
.title{
  color: #afb6d3;
  font-family: "PingFangSC-Medium";
  font-size: 32rpx;
  font-weight: 400;
  text-align: center;
  margin-top: 20rpx;
  margin-left: 266rpx;
}
.btn_desc{
  width: 136rpx;
  height: 60rpx;
  background: #5276f2;
  border-radius: 50rpx;
  line-height: 60rpx;
  text-align: center;
  color: #fff;
  margin-top: 18rpx;
  margin-left: 170rpx;
}
/* 字体图标 */
.icon-x:before {
  content: "\e903";
}
.icon-cancel{
		font-family: 'iconfont';
		font-style: normal;
		width: 20rpx;
		height: 20rpx;
		position: absolute;
		left: 44rpx;
		top:  24rpx;
		color:#afb6d3
}
/* 弹窗-正文  */
.contentDesc{
  color: #071756;
  font-family: "PingFangSC-Regular";
  font-size: 32rpx;
  font-weight: 400;
  padding: 40rpx 40rpx 0;
  box-sizing: border-box;
  height: 100%;
  width: 100%;
  word-wrap: break-word;
  word-break: normal;
}
.bot_tt{
  position: fixed;
  bottom: 0rpx;
  right: 20rpx;
  color: #ccc;
  font-size: 32rpx;
}
/* 字体图标 */
.icon-chevron-right:before {
  content: "\e902";
}
.icon-right{
  position: absolute;
  top: 70rpx;
  right: 40rpx;
}
.icon-right-spc{
  top: 30rpx;
}
/* 二维码提示框 */
.code_class{
  width: 310rpx;
  height: 88rpx;
  background-color:  #5276f2;
  position: absolute;
  top: 0rpx;
  left: 30%;
  font-size: 28rpx;
  color: #fff;
  line-height: 88rpx;
  text-align: center;
  border-radius: 60rpx;
}
/* 退出提示框 */
.hihtID{
  width: 540rpx;
  height: 400rpx;
  background: rgba(174, 173, 173, 0.1);
  border-radius: 20rpx;
  margin: 0 auto;
  margin-top: 40rpx;
  color: #071756;
  font-size: 32rpx;
  position: absolute;
  top: 23%;
  left: 90rpx;
  padding: 60rpx 50rpx 30rpx;
  box-sizing: border-box;
}
.h1{
  font-size: 40rpx;
  text-align: center;
}
.h2{
  margin-top: 30rpx;
  padding-bottom: 40rpx;
  border-bottom: 1rpx solid #ccc;
  padding-bottom: 70rpx;
}
.H4{
  display: flex;
  line-height: 118rpx;
}
.err{
  height: 106rpx;
  padding-right: 85rpx;
  border-right: 1rpx solid #ccc;
  color: #ccc;
  margin-left: 50rpx;
}
.yes{
  margin-left: 98rpx;
}