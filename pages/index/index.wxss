@font-face {
  font-family: 'icomoon';
  src: url('data:font/truetype;charset=utf-8;base64,') format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
[class^="icon-"], [class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'icomoon' !important;
  /* speak: never; */
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.content{
		height: 37.167rem;
		text-align: center;
		position: relative;
}
/* 登录页面 */
.top{
		height: 64rpx;
		text-align: center;
		margin-top: 5rpx;
		line-height: 64rpx;
}
/* 字体图标 */
.icon-x:before {
  content: "\e903";
}
.icon-cancel{
		font-family: 'iconfont';
		font-style: normal;
		width: 1.667rem;
		height: 1.667rem;
		position: absolute;
		left: 1.042rem;
		top: 0.625rem;
		color:#afb6d3
}
.title {
		width: 64rpx;
		height: 40rpx;
		color: #afb6d3;
		font-family: "PingFangSC-Medium";
		font-size: 32rpx;
		font-weight: 400;
		line-height: 20rpx;
}
.logo{
		width: 126rpx;
		height: 114rpx;
		margin-left: 320rpx;
		background: url(data:image/png;base64,)no-repeat center;
}
.center{
		margin-top: 120rpx;
}
.center-tel{
		width: 320rpx;
		height: 60rpx;
		color: #071756;
		font-family: "PingFangSC-Medium";
		font-size: 40rpx;
		font-weight: 400;
		line-height: 60rpx;
}
.center-agree{
		width: 239rpx;
		height: 16rpx;
		color: #afb6d3;
		font-family: "PingFangSC-Regular";
		font-size: 24rpx;
		font-weight: 400;
		line-height: 16rpx;
		padding-bottom: 30rpx;
}
.center-num{
		 width: 237rpx;
		height: 51rpx;
		color: #5276f2;
		font-family: "PingFangSC-Medium";
		font-size: 80rpx;
		font-weight: 400;
		line-height: 160rpx;
}
.spilce{
		color: #5276f2;
}
.button{
	position: absolute;
	bottom: 0rpx;
	left: 1.042rem;
}
.btn{
	padding: 0;
	height: 72rpx;
	background-color: #5276f2;
	color: #fff;
	font-family: "PingFangSC-Medium";
	font-size: 28rpx;
	font-weight: 400;
	line-height: 72rpx;
	text-align: center;
	border-radius: 36rpx;
}
/* 登录页面end */

